{
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist",
    "dev-api": "nodemon $NODE_DEBUG_OPTION src/main/index.js --exec babel-node",
    "dev-api-win": "nodemon src/main/index.js --exec babel-node",
    "start": "node dist/main/index.js",
    "test-api": "PORT=9000 mocha --exit $(find src/test -name '*.js') --require babel-register",
    "test-api-win": "set PORT=9000 && mocha --recursive --require babel-register --exit \"src/test/**/*.js\"",
    "test-init-db-win": "mocha --exit src/test/util/testInitDb.js --require babel-register",
    "test-init-db": "mocha --exit src/test/util/testInitDb.js --require babel-register",
    "upgrade-query": "yarn upgrade -L trees-query",
    "upgrade-items-service": "yarn upgrade -L trees-items-service",
    "upgrade-units": "yarn upgrade -L trees-units",
    "yarn": "yarn"
  },
  "env": "api",
  "version": "0.1.1",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.17.3",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "sinon": "^4.2.1"
  },
  "dependencies": {
    "args-parser": "^1.1.0",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "express-validator": "^5.1.2",
    "fraction.js": "^4.0.4",
    "fs-readdir-recursive": "^1.1.0",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.33",
    "morgan": "^1.9.0",
    "nested-error-stacks": "^2.0.0",
    "pick-to-array": "^1.1.0",
    "regex-escape": "^3.4.8",
    "trees-items-service": "^0.1.14",
    "trees-query": "^0.0.9",
    "trees-units": "^1.0.11"
  },
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime",
      [
        "transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
          ]
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": []
      }
    }
  }
}
