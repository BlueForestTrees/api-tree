{
  "name": "api-tree",
  "version": "0.0.3",
  "scripts": {
    "start": "node dist/index.js",
    "build": "rimraf dist && babel src --out-dir dist/api-tree/js && copyfiles -u 1 templates/* dist/etc/api-tree/templates",
    "dev-api": "node --version && cross-env DB_UPGRADE=LAST MAIL_CONFIG_PATH=../secured/api-tree nodemon src/index.js --exec babel-node",
    "test-api": "cross-env NODE_ENV=test PORT=9000 MAIL_CONFIG_PATH=../secured/api-tree mocha --recursive --require babel-register --exit \"test/**/*.js\"",
    "test-init-db": "cross-env NODE_ENV=test MAIL_CONFIG_PATH=../secured/api-tree mocha --exit test/scenarios/tu/testInitDb.js --require babel-register"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chai-http": "3.0.0",
    "copyfiles": "2.0.0",
    "jsonpath": "1.0.0",
    "mocha": "5.0.0",
    "nodemon": "1.17.3",
    "nyc": "11.4.1",
    "rimraf": "2.6.2",
    "sinon": "4.2.1",
    "api-test": "file:../api-test"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "compare-versions": "3.3.0",
    "cookie-parser": "~1.4.3",
    "cross-env": "5.2.0",
    "debug": "~2.6.9",
    "exceljs": "1.5.1",
    "express-fileupload": "0.4.0",
    "express-validator": "5.3.0",
    "fraction.js": "4.0.4",
    "fs-readdir-recursive": "1.1.0",
    "handlebars": "4.0.11",
    "jsonwebtoken": "8.3.0",
    "lodash": "4.17.10",
    "morgan": "1.9.0",
    "nested-error-stacks": "2.0.0",
    "nodemailer": "4.6.7",
    "pick-to-array": "1.1.0",
    "regex-escape": "3.4.8",
    "sha1": "1.1.1",
    "mongo-registry": "1.0.0",
    "items-service": "1.0.0",
    "mongo-queries-blueforest": "1.0.0",
    "unit-manip": "1.0.1",
    "express-blueforest": "1.0.2"
  },
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime",
      [
        "transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
          ]
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": []
      }
    }
  }
}
