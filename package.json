{
  "scripts": {
    "dev-api": "nodemon $NODE_DEBUG_OPTION src/main/index.js --exec babel-node",
    "build-api": "babel lib -d dist",
    "serve-api": "node dist/index.js",
    "test-api": "NODE_ENV=test mocha --exit $(find src/test -name '*.js') --require babel-register",
    "test-init-db": "NODE_ENV=test mocha --exit src/test/testInitDb.js --require babel-register"
  },
  "env": "api",
  "version": "0.0.1-SNAPSHOT",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.11",
    "nyc": "^11.4.1",
    "sinon": "^4.2.1"
  },
  "dependencies": {
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "express-validator": "^4.3.0",
    "fraction.js": "^4.0.4",
    "fs-readdir-recursive": "^1.1.0",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.33",
    "morgan": "^1.9.0",
    "nested-error-stacks": "^2.0.0",
    "pick-to-array": "^1.1.0"
  },
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime",
      [
        "transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
          ]
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": []
      }
    }
  }
}
